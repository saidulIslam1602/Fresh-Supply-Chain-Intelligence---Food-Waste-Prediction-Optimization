{
  "dashboard": {
    "id": null,
    "uid": "supply-chain-overview",
    "title": "Fresh Supply Chain Intelligence - Executive Overview",
    "description": "Executive dashboard for Fresh Supply Chain Intelligence System with real-time KPIs and business metrics",
    "tags": ["fresh-supply", "executive", "kpi", "business"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"fresh-supply-api\"}",
            "legendFormat": "API",
            "refId": "A"
          },
          {
            "expr": "up{job=\"fresh-supply-dashboard\"}",
            "legendFormat": "Dashboard",
            "refId": "B"
          },
          {
            "expr": "up{job=\"sqlserver\"}",
            "legendFormat": "Database",
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Cache",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ],
            "unit": "none"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal"
        }
      },
      {
        "id": 2,
        "title": "OTIF Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "business_otif_rate * 100",
            "legendFormat": "OTIF %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 4, "w": 3, "x": 6, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "Temperature Compliance",
        "type": "stat",
        "targets": [
          {
            "expr": "business_temperature_compliance_rate * 100",
            "legendFormat": "Compliance %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 4, "w": 3, "x": 9, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 4,
        "title": "Waste Reduction",
        "type": "stat",
        "targets": [
          {
            "expr": "business_waste_reduction_percentage * 100",
            "legendFormat": "Reduction %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 15},
                {"color": "green", "value": 25}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 4, "w": 3, "x": 12, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 5,
        "title": "AI Model Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(model_prediction_accuracy) * 100",
            "legendFormat": "Accuracy %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 4, "w": 3, "x": 15, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 6,
        "title": "Monthly Cost Savings",
        "type": "stat",
        "targets": [
          {
            "expr": "business_monthly_cost_savings",
            "legendFormat": "Savings NOK",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 75000},
                {"color": "green", "value": 100000}
              ]
            },
            "unit": "currencyNOK",
            "decimals": 0
          }
        },
        "gridPos": {"h": 4, "w": 3, "x": 18, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 7,
        "title": "API Performance Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(api_requests_total{job=\"fresh-supply-api\"}[5m])",
            "legendFormat": "Requests/sec - {{method}} {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket{job=\"fresh-supply-api\"}[5m]))",
            "legendFormat": "95th Percentile Latency",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "Requests.*"},
              "properties": [
                {"id": "unit", "value": "reqps"},
                {"id": "custom.axisPlacement", "value": "right"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["last", "max"]}
        }
      },
      {
        "id": 8,
        "title": "Error Rate & Status Codes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(api_requests_total{job=\"fresh-supply-api\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors/sec",
            "refId": "A"
          },
          {
            "expr": "rate(api_requests_total{job=\"fresh-supply-api\",status=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors/sec",
            "refId": "B"
          },
          {
            "expr": "rate(api_requests_total{job=\"fresh-supply-api\",status=~\"2..\"}[5m])",
            "legendFormat": "2xx Success/sec",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["last", "max"]}
        }
      },
      {
        "id": 9,
        "title": "Business KPI Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "business_otif_rate",
            "legendFormat": "OTIF Rate",
            "refId": "A"
          },
          {
            "expr": "business_temperature_compliance_rate",
            "legendFormat": "Temperature Compliance",
            "refId": "B"
          },
          {
            "expr": "business_waste_reduction_percentage",
            "legendFormat": "Waste Reduction",
            "refId": "C"
          },
          {
            "expr": "avg(model_prediction_accuracy)",
            "legendFormat": "AI Accuracy",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["last", "min", "max", "mean"]}
        }
      },
      {
        "id": 10,
        "title": "ML Model Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "model_prediction_accuracy{model_type=\"vision\"}",
            "legendFormat": "Vision Model Accuracy",
            "refId": "A"
          },
          {
            "expr": "model_forecast_mape{model_type=\"forecasting\"} / 100",
            "legendFormat": "Forecasting MAPE",
            "refId": "B"
          },
          {
            "expr": "avg_over_time(model_prediction_uncertainty{model_type=\"vision\"}[5m])",
            "legendFormat": "Vision Model Uncertainty",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit",
            "min": 0,
            "max": 1
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*MAPE.*"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "right"},
                {"id": "unit", "value": "percent"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["last", "mean"]}
        }
      },
      {
        "id": 11,
        "title": "Resource Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"fresh-supply-api\"}[5m]) * 100",
            "legendFormat": "API CPU %",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"fresh-supply-api\"} / (1024*1024*1024)",
            "legendFormat": "API Memory GB",
            "refId": "B"
          },
          {
            "expr": "redis_memory_used_bytes{job=\"redis\"} / (1024*1024*1024)",
            "legendFormat": "Redis Memory GB",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*CPU.*"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "max", "value": 100}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*Memory.*"},
              "properties": [
                {"id": "unit", "value": "decgbytes"},
                {"id": "custom.axisPlacement", "value": "right"}
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "values": ["last", "max"]}
        }
      },
      {
        "id": 12,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": false, "displayName": "severity"}]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "Value": true, "__name__": true},
              "indexByName": {
                "alertname": 0,
                "severity": 1,
                "component": 2,
                "instance": 3,
                "summary": 4
              },
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "component": "Component",
                "instance": "Instance"
              }
            }
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "warehouse",
          "type": "query",
          "query": "label_values(business_otif_rate, warehouse)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "model_type",
          "type": "query",
          "query": "label_values(model_prediction_accuracy, model_type)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "increase(deployment_timestamp[1m])",
          "titleFormat": "Deployment",
          "textFormat": "New deployment: {{version}}",
          "iconColor": "blue"
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus", 
          "expr": "ALERTS{alertstate=\"firing\"}",
          "titleFormat": "Alert: {{alertname}}",
          "textFormat": "{{summary}}",
          "iconColor": "red"
        }
      ]
    },
    "links": [
      {
        "title": "API Documentation",
        "url": "http://api:8000/docs",
        "type": "link",
        "icon": "doc",
        "tooltip": "API Documentation",
        "targetBlank": true
      },
      {
        "title": "Business Dashboard",
        "url": "http://dashboard:3000",
        "type": "link",
        "icon": "dashboard",
        "tooltip": "Business Intelligence Dashboard",
        "targetBlank": true
      }
    ]
  }
}